ios Services\nimport {getAllMovieData} from '../services/movieServices.js';\n\n\n  const useStyles = makeStyles((theme) => ({\n    pagination: {\n        paddingTop: \"25px\",\n        alignSelf: \"center\",\n        \"@media (max-width: 330px)\": {\n            maxWidth: \"300px\",\n         },\n    },\n    paginationItem: {\n        fontSize: \"0.2em\",\n    },\n    }));\n\nfunction AllMovieListings({actions, allMovieDataPerPage}) {\n   const classes = useStyles();\n   //const { pagination, paginationItem} = useStyles();\n   const [isLoaded, setisLoaded] = useState(false)\n   const [pageCount, setPageCount] = useState(500)\n   const [currentPage, setCurrentPage] = useState(1);\n   const [movieData, setMovieData] = useState([]);\n\n   //function here that checks Redux for watched movies\n\n\n   //Function which calls the Movie API returning all Movies, and paginating the data.\n   const handleAxiosData = async () => {\n    setisLoaded(false)\n    await getAllMovieData(currentPage)\n       .then((res) => {\n           //actions.getMoviesDisplay(res.results)\n\t       setMovieData(res.results);\n           setPageCount(res.total_pages)\n           //console.log(\"Success\", res.results)\n           console.log(\"Page\", res.page)\n           setisLoaded(true)\n          })\n          .catch((error) => {\n            console.log(\"Error\", error.response)\n          })\n\t  };\n\n\tconst handlePageChange = (event, selectedObject) => {\n        setCurrentPage(selectedObject);\n\t};\n\n   useEffect(() => {\n      handleAxiosData()\n   }, [currentPage]);\n\n  return (\n        <BaseContainer>\n           <UnderNavHeader location=\"allMovies\"/>\n            {isLoaded ? (\n               <>\n                <div className={classes.pagination}>\n                     <Pagination\n                        className={classes.paginationItem}\n                        count={pageCount}\n                        page={currentPage}\n                        onChange={handlePageChange}\n                        boundaryCount={1}\n\n                    />\n                </div>\n                <MovieContainer>\n                            {movieData.map((movie)=>\n                                <MovieCard key={movie.id} movie={movie}/>\n                            )\n                            }\n                 </MovieContainer>\n                </>\n                ) : (\n\t\t\t\t<div><Loading/></div>\n\t\t\t    )}\n            </BaseContainer>\n  );\n}\n\n\nconst mapStateToProps = (state) => ({\n  allMovieDataPerPage: state.movies.allMovieData,\n});\n\n\nconst mapDispatchToProps = (dispatch) => ({\n  actions: {\n   getAllMoviesPerPage: (movies) => {\n      dispatch({ type: \"getAllMoviesPerPage\", payload: movies });\n    },\n\n  },\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(AllMovieListings);\n\n","/Users/shelbyd/CODING/coding-challenges/martins-movies/src/components/loading.js",[],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/components/underNavheader.js",["74","75"],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/pages/searchedMovies.js",["76","77"],{"ruleId":"78","severity":1,"message":"79","line":34,"column":14,"nodeType":"80","messageId":"81","endLine":34,"endColumn":26},{"ruleId":"78","severity":1,"message":"82","line":12,"column":10,"nodeType":"80","messageId":"81","endLine":12,"endColumn":17},{"ruleId":"78","severity":1,"message":"83","line":12,"column":19,"nodeType":"80","messageId":"81","endLine":12,"endColumn":29},{"ruleId":"78","severity":1,"message":"84","line":10,"column":10,"nodeType":"80","messageId":"81","endLine":10,"endColumn":23},{"ruleId":"78","severity":1,"message":"85","line":35,"column":14,"nodeType":"80","messageId":"81","endLine":35,"endColumn":29},{"ruleId":"86","severity":1,"message":"87","line":83,"column":10,"nodeType":"88","endLine":83,"endColumn":59},{"ruleId":"89","severity":1,"message":"90","line":65,"column":7,"nodeType":"91","endLine":65,"endColumn":20,