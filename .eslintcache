2.find(element => {\n              return element === el.id;\n            });\n        });\n        //return res;\n        const displayGens = []\n        res.map((genre)=>{\n            displayGens.push(genre.name)\n        })\n        return displayGens.join(',  ');\n        //setGenreState(displayGens)\n    }\n\n\n\n    function handleChecker (id){\n      if (checked === true){\n          setChecked(false)\n          console.log(\"unmarked\")\n          actions.markWatched(id)\n      } else {\n          setChecked(true)\n          console.log(\"marked\")\n          actions.unMarkWatched(id)\n      }\n    }\n\n\n   function handleCheckerSubmit (){\n      if (checked === true){\n          actions.markWatched(id)\n      } else {\n          actions.unMarkWatched(id)\n      }\n    }\n    // function markedMovieHandler (){\n    //   actions.markWatched\n    // }\n    // function unmarkedMovieHandler (){\n    //   actions.unMarkWatched\n    // }\n\n   //console.log(filterByReference(genresArray, genre_ids));\n\n  return (\n\n      <MovieCardStyle key={id}>\n        <Link to={\"/#\"} style={{ textDecoration: 'none' }} >\n\n                       {poster_path ?\n                       <img alt=\"movie poster\" src={`https://image.tmdb.org/t/p/original${poster_path}`} width=\"100%\" />\n                       :\n                       <img alt=\"movie poster\" src=\"data:image/png;base64,\" width=\"100%\" />\n                       }\n\n                      <h2>{title ? title : name}</h2>\n                      <div>\n                        <p><img alt=\"star rating\" src=\"https://img.icons8.com/fluent/48/000000/filled-star.png\" width=\"15px\" style={{marginRight: \"5px\" }}/>\n                        {vote_average} / 10</p>\n                        <p>{filterByReference(genresArray, genre_ids)}</p>\n                      </div>\n                      <div class=\"overview\">{overview}</div>\n\n     </Link>\n\n     <div class=\"buttonGroup\" >\n         <a to={\"/#\"} style={{ textDecoration: 'none' }} >\n              <ReadMoreButton>Read More</ReadMoreButton>\n          </a>\n            <Watched>\n            <input\n                  name=\"isWatched\"\n                  type=\"checkbox\"\n                  checked={checked}\n                  onChange={handleChecker}\n                  />\n            <span>Unwatched</span>\n          </Watched>\n     </div>\n\n\n   </MovieCardStyle>\n\n  );\n}\n\n\nconst mapStateToProps = (state) => ({\n  listOfMovies: state.movies.movieData,\n  listOfWatchedMovies: state.movies.watchedMovies,\n});\n\n\nconst mapDispatchToProps = (dispatch) => ({\n  actions: {\n    markWatched: (movieId) => {\n      dispatch({ type: \"addTooWatchedMovies\", payload: movieId });\n    },\n    unMarkWatched: (movieId) => {\n      dispatch({ type: \"deleteWatchedMovies\", payload: movieId });\n    },\n  },\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(MovieCard);\n\n\n\n","/Users/shelbyd/CODING/coding-challenges/martins-movies/src/App.js",[],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/pages/404.js",[],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/pages/allMovies.js",["74","75"],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/components/loading.js",[],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/components/underNavheader.js",["76","77"],"/Users/shelbyd/CODING/coding-challenges/martins-movies/src/pages/searchedMovies.js",["78","79"],{"ruleId":"80","severity":1,"message":"81","line":33,"column":14,"nodeType":"82","messageId":"83","endLine":33,"endColumn":26},{"ruleId":"80","severity":1,"message":"84","line":12,"column":10,"nodeType":"82","messageId":"83","endLine":12,"endColumn":17},{"ruleId":"80","severity":1,"message":"85","line":12,"column":19,"nodeType":"82","messageId":"83","endLine":12,"endColumn":29},{"ruleId":"80","severity":1,"message":"86","line":10,"column":10,"nodeType":"82","messageId":"83","endLine":10,"endColumn":23},{"ru